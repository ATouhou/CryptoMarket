{
    "name": "algomateinc/cryptomarket",
    "description": "PHP package defining a unified interface for interacting with cryptocurrency exchanges",
    "license": "Apache-2.0",
    "keywords": ["cryptocurrency", "bitcoin", "trading", "exchange"],
    "require": {
        "mongodb/mongodb": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
          "CryptoMarket\\": "src",
          "CryptoMarket\\AccountLoader\\": "src/accountloader",
          "CryptoMarket\\Account\\": "src/account",
          "CryptoMarket\\Exchange\\": "src/exchange",
          "CryptoMarket\\Helper\\": "src/helper",
          "CryptoMarket\\Record\\": "src/record"
        }
    }
}
